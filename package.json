    "express-handlebars": "^7.1.2",
    "handlebars": "^4.7.8",
    "mysql": "^2.18.1"
  },
  "devDependencies": {
    "clean-css-cli": "^5.6.3",
    "terser": "^5.44.0"
  }
}
